let nome='Aristarco'


if (1 > 0) {
    let nome = 'Irabor';
    console.log(nome);
}

function nome(){
    let nome='georgina'
    return nome
}

console.log(nome)
let teste=nome()
console.log(teste)

/*"Namespace" (espaço de nomes) 
No JavaScript, o nome de uma função e o nome de uma variável ocupam o mesmo espaço na memória. 
Se você usa o mesmo nome para ambos, o motor do JS entra em conflito porque não sabe se você 
está tentando chamar uma ação ou ler um dado*/
